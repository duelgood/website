# /etc/postfix/main.cf
myhostname = mail.duelgood.org
mydestination = localhost
inet_interfaces = all
mynetworks = 127.0.0.0/8 [::ffff:127.0.0.0]/104 [::1]/128

# Forward all duelgood.org emails to your Gmail
virtual_alias_maps = hash:/etc/postfix/virtual
relay_domains = duelgood.org

# Log to stdout
maillog_file = /dev/stdout